config QSD_AUDIO
	bool "QSD audio"
	depends on ARCH_MSM_SCORPION && MSM_DALRPC && ANDROID_PMEM
	default y
	help
	  Provides PCM, MP3, and AAC audio playback.

config AUDIO_AAC_PLUS
	depends on (MSM_ADSP || QSD_AUDIO || MSM7KV2_AUDIO)
	bool "AAC+ Audio"
	default y
	help
	  Provides AAC+ decoding

config AUDIO_ENHANCED_AAC_PLUS
	depends on AUDIO_AAC_PLUS
	bool "Enhanced AAC+ Audio"
	default y
	help
	  Provides Enhanced AAC+ decoding

config SURF_FFA_GPIO_KEYPAD
	bool "MSM SURF/FFA GPIO keypad"
	depends on INPUT_GPIO = "y"
	default y
	help
	  Select if the GPIO keypad is attached.

config CLOCK_BASED_SLEEP_LIMIT
	default y
	bool "Set sleep limitation based on clock usage"
	help
	  The application processor checks for enabled clocks and
	  decides accordingly the sleep limitation which it informs
	  the modem to use.

config MSM_SLEEP_TIME_OVERRIDE
	bool "Allow overriding suspend/sleep time with PM module parameter"
	default y
	help
	  Enable the module parameter sleep_time_override. Specified
	  in units of seconds, it overwrites the normal sleep time of
	  suspend. The feature is required for automated power management
	  testing.

choice
	prompt "Power management timeout action"
	default MSM_PM_TIMEOUT_HALT
	help
	  Selects the Application Processor's action when Power Management
	  times out waiting for Modem's handshake.

	config MSM_PM_TIMEOUT_HALT
		bool "Halt the Application Processor"

	config MSM_PM_TIMEOUT_RESET_MODEM
		bool "Reset the Modem Processor"

	config MSM_PM_TIMEOUT_RESET_CHIP
		bool "Reset the entire chip"
endchoice

config SEMC_SEPORT_PLATFORM
	bool "SEMC Seport platform driver"
	depends on (ARCH_SQD || ARCH_MSM) && SEMC_SEPORT
	default n
	help
	  hardware dependant driver for SEMC Seport systemconnector.
	  Requires SEMC systemconnector to be enabled!

config SEMC_POWER
	tristate "Semc power"
	depends on POWER_SUPPLY
	default n
	help
	  Enable semc battery power manager.

config DELTA_PROXIMITY_SENSOR
	tristate "Proximity sensor driver for delta platform"
	default y
	select SEMCCLASS
	depends on (ARCH_SQD || ARCH_MSM)
	help
	  This option enables support delta platform
	  proximity sensor.

config TI1271
	bool "Use connectivity driver and config for TI1271"
	default n
	help
	  This option enables support for TI1271.

config MSM_MISC_MODEM_API
	bool "MSM misc modem  API"
	depends on (ARCH_SQD || ARCH_MSM)
	default y
	help
	  This option enables misc modem API driver

config ES209RA_HEADSET
	bool "ES209RA headset detection driver"
	default y if MACH_ES209RA
	help
	  Provides support for detecting 3.5mm audio jack, such as wired
	  headset, on the es209ra platform.

config NPA_REMOTE_ONCRPC
	bool "Enables NPA remoting through ONCRPC protocol."
	depends on NPA && MSM_ONCRPCROUTER
	select NPA_REMOTE
	default n
	help
	  Enables Linux NPA clients to talk to NPA nodes in the modem
	  using ONCRPC communication protocol.

config ES209RA_INPUT_GPIO_MATRIX
	bool "ES209RA GPIO key matrix driver"
	depends on MACH_ES209RA && INPUT_GPIO
	default y
	help
	  Enable GPIO key matrix driver for es209ra.
	  This driver is derived from common GPIO key matrix driver(gpio_matrix.c)
	  Difference from the common driver is as follows:
	   1. Support discard timer for long key press.
	   2. Pull-up the KR line when the other KR line is being driven.

config AUDIO_QDSP_AMP_GPIO_CTRL
	bool "Enable audio qdsp amp gpio ctrl"
	default n
	help
	  Enable GPIO control of audio qdsp amplifier.

config MSM_SPM
	bool "Subsystem Power Manager"
	depends on ARCH_MSM7X30
	default y
	help
	  Enables support for the subsystem power manager.

config EXP_DABORT_MSG
	bool "Expand Data Abort Message"
	default n
	depends on ARCH_MSM_SCORPION
	help
	  This enables the expanded Oops message when data abort occurred.

config SEMC_LOW_BATT_SHUTDOWN
	bool "Low-Battery Shutdown support"
	depends on (ARCH_MSM && MSM_ONCRPCROUTER)
	default n
	help
	  Say Y to enable support for the low-battery detection.
	  This driver communicate with AMSS VBatt driver.
	  When battery voltage is less than the registerd threashold
	  during power collapse, AMSS wakes ACPU up and sends callback
	  request to this function.

config SEMC_RPC_SERVER_HANDSET
       bool "Enable SEMC handset rpc dispatcher"
       depends on (ARCH_SQD || ARCH_MSM) && MSM_ONCRPCROUTER
       default n
       help
         This enables SEMC version of rpc_server_handset message
	 dispatcher.
	 This cannot cooperate with MSM_RPCSERVER_HANDSET. If this
	 is set, an error will be generated.

config PMIC_TIME
	bool "Export pmic time as sysfs entry"
	depends on (ARCH_MSM)
	default n
	help
	  This exports pmic time in a sysfs entry.
endif
