config MSM_STACKED_MEMORY
	bool "Stacked Memory"
	default y
	help
	  This option is used to indicate the presence of on-die stacked
	  memory.  When present this memory bank is used for a high speed
	  shared memory interface.  When not present regular RAM is used.

config PHYS_OFFSET
	hex
	default "0x00200000" if !MSM_STACKED_MEMORY && !CRASH_DUMP
	default "0x0D200000" if !MSM_STACKED_MEMORY && ARCH_MSM7X27 && CRASH_DUMP
	default "0x00000000" if ARCH_QSD8X50 && MSM_SOC_REV_A
	default "0x20000000" if ARCH_QSD8X50 && !CRASH_DUMP
	default "0x37800000" if ARCH_QSD8X50 && CRASH_DUMP
	default "0x10000000"

